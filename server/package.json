{
  "name": "server",
  "version": "0.1.0",
  "description": "Caparnaum servers",
  "author": "Tom Nurkkala",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "load-fake-survey": "ts-node fixtures/fixture-loader.ts --nuclear --fake-survey",
    "load-fixtures": "ts-node fixtures/fixture-loader.ts --nuclear",
    "reporter": "nest start reporter",
    "reporter:build": "nest build reporter",
    "reporter:debug": "nest start --debug --watch reporter",
    "reporter:dev": "nest start --watch reporter",
    "server": "nest start server",
    "server:build": "nest build server",
    "server:debug": "nest start --debug --watch server",
    "server:debug:webstorm": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./src/main.ts",
    "server:dev": "nest start --watch server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/alpha-main/test/jest-e2e.json",
    "test:load": "artillery run --output load-test.json apps/server/test/load/load.yaml",
    "test:load:dev": "artillery run --output load-test.json apps/server/test/load/load-dev.yaml",
    "test:profile": "tsc-watch -p tsconfig.build.json --onSuccess \"node --prof dist/main.js\"",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/bull": "^0.2.3",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/graphql": "^7.8.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/typeorm": "^7.1.4",
    "@willsoto/nestjs-prometheus": "^2.0.1",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "bull": "^3.18.1",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "got": "^11.8.0",
    "graphql": "15",
    "graphql-tools": "^7.0.1",
    "html-to-text": "^6.0.0",
    "isemail": "^3.2.0",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "nodemailer": "^6.4.15",
    "numbro": "^2.3.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.2",
    "pify": "^5.0.0",
    "pluralize": "^8.0.0",
    "prom-client": "^12.0.0",
    "quill-delta": "^4.2.2",
    "quill-delta-to-html": "^0.12.0",
    "raw-body": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "table": "^6.0.4",
    "tslib": "^2.0.3",
    "tunnel": "^0.0.6",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.2",
    "@nestjs/testing": "^7.5.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.14.4",
    "@types/chance": "^1.1.1",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^5.1.5",
    "@types/html-to-text": "^5.1.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/nunjucks": "^3.1.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/pluralize": "^0.0.29",
    "@types/raw-body": "^2.3.0",
    "@types/supertest": "^2.0.10",
    "@types/table": "^6.0.0",
    "@types/uuid": "^8.3.0",
    "@types/yauzl": "^2.9.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "artillery": "^1.6.1",
