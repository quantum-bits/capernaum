    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "html-to-text": "^5.1.1",
    "https-proxy-agent": "^4.0.0",
    "lodash": "^4.17.15",
    "luxon": "^1.21.2",
    "nodemailer": "^6.4.2",
    "numbro": "^2.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.14.0",
