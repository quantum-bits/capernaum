{
  "name": "server",
  "version": "0.1.0",
  "description": "Capernaum servers",
  "author": "Tom Nurkkala",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "load-fixtures": "ts-node fixtures/main.ts",
    "reporter": "nest start reporter",
    "reporter:build": "nest build reporter",
    "reporter:debug": "nest start --debug --watch reporter",
    "reporter:dev": "nest start --watch reporter",
    "server": "nest start server",
    "server:build": "nest build server",
    "server:debug": "nest start --debug --watch server",
    "server:debug:webstorm": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./src/main.ts",
    "server:dev": "nest start --watch server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/server/test/jest-e2e.json",
    "test:load": "artillery run --output load-test.json apps/server/test/load/load.yaml",
    "test:load:dev": "artillery run --output load-test.json apps/server/test/load/load-dev.yaml",
    "test:profile": "tsc-watch -p tsconfig.build.json --onSuccess \"node --prof dist/main.js\"",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.6.17",
    "@nestjs/core": "^7.6.17",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/typeorm": "^7.1.5",
    "@willsoto/nestjs-prometheus": "^3.0.0",
    "apollo-server-express": "^2.24.1",
    "bcrypt": "^5.0.1",
    "bull": "^3.22.6",
    "chalk": "^4.1.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "debug": "^4.3.1",
    "dotenv": "^9.0.2",
    "gluegun": "^4.6.1",
    "got": "^11.8.2",
    "graphql": "15",
    "graphql-tools": "^7.0.5",
    "handlebars": "^4.7.7",
    "html-to-text": "^7.1.1",
    "isemail": "^3.2.0",
