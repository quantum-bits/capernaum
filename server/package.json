    "pluralize": "^8.0.0",
    "prom-client": "^13.1.0",
    "quill-delta": "^4.2.2",
    "quill-delta-to-html": "^0.12.0",
    "raw-body": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.1",
    "table": "^6.7.1",
    "tslib": "^2.2.0",
    "tunnel": "^0.0.6",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
