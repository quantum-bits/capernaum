{
  "name": "server",
  "version": "0.1.0",
  "description": "Capernaum servers",
  "author": "Tom Nurkkala",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "load-fixtures": "DEBUG=cap:* ts-node -r tsconfig-paths/register fixture-loader/main.ts",
    "cli": "node ./dist/apps/cli/cap-cli.js",
    "cli:why": "node -r why-is-node-running/include ./dist/apps/cli/cap-cli.js",
    "cli:build": "nest build cli",
    "cli:dev": "nest build cli --watch",
    "reporter": "nest start reporter",
    "reporter:build": "nest build reporter",
    "reporter:debug": "nest start --debug --watch reporter",
    "reporter:dev": "nest start --watch reporter",
    "server": "nest start server",
    "server:build": "nest build server",
    "server:debug": "nest start --debug --watch server",
    "server:debug:webstorm": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./src/main.ts",
    "server:dev": "nest start --watch server",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create --connection=staging",
    "migration:generate": "yarn typeorm migration:generate --connection=staging",
    "migration:run": "yarn typeorm migration:run --connection=staging",
    "migration:show": "yarn typeorm migration:show --connection=staging",
    "migration:revert": "yarn typeorm migration:revert --connection=staging",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/server/test/jest-e2e.json",
    "test:load": "artillery run --output load-test.json apps/server/test/load/load.yaml",
    "test:load:dev": "artillery run --output load-test.json apps/server/test/load/load-dev.yaml",
    "test:profile": "tsc-watch -p tsconfig.build.json --onSuccess \"node --prof dist/main.js\"",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.9",
