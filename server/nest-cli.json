{
  "language": "ts",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "apps/server",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/server/tsconfig.app.json"
  },
  "projects": {
    "server": {
      "type": "application",
      "root": "apps/server",
      "entryFile": "main",
      "sourceRoot": "apps/server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/server/tsconfig.app.json",
        "assets": ["**/group/letters/*.txt", "**/group/letters/*.html"]
      }
    },
    "reporter": {
      "type": "application",
      "root": "apps/reporter",
      "entryFile": "main",
      "sourceRoot": "apps/reporter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reporter/tsconfig.app.json"
      }
    },
    "qualtrics-api": {
      "type": "library",
      "root": "libs/qualtrics-api",
      "entryFile": "index",
      "sourceRoot": "libs/qualtrics-api/src",
      "compilerOptions": {
        "tsConfigPath": "libs/qualtrics-api/tsconfig.lib.json"
      }
    }
  }
}
