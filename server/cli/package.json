{
  "name": "capcli",
  "description": "Capernaum server CLI",
  "version": "0.0.0",
  "author": "Tom Nurkkala",
  "bin": {
    "capcli": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "table": "^5.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^10",
    "@types/table": "^4.0.5",
    "globby": "^8",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "capcli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./lib/hooks/init/env"
      ]
    }
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
